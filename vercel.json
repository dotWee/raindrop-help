{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/pb/js/script.js",
            "destination": "https://plausible.io/js/script.js"
        },
        {
            "source": "/pb/api/event",
            "destination": "https://plausible.io/api/event"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=365000000, immutable"
                }
            ]
        }
    ]
}